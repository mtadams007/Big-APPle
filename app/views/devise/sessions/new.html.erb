<%= render "devise/shared/login-signup" %>

<div class="login-box"> 
  <h2 id="login-header">Log in</h2> 
 
  <%= simple_form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %> 
    <%= f.input :email, label: false, placeholder: 'Enter email' %> 
    <%= f.input :password, label: false, placeholder: 'Password' %> 
    <%= f.button :submit, "Log In", class: "login-button" %> 
  <% end %>

  <%= render "devise/shared/links" %> 

</div>

